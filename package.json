    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "toastr": "^2.1.4",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "laravel-vue-pagination": "^2.3.1",
    "moment": "^2.24.0",
    "toastr": "^2.1.4",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.1.3",
    "vue-sweetalert2": "^2.1.1",
    "vue-toastr-2": "0.0.10",
    "vuejs-loading-plugin": "^1.0.51"
  }
